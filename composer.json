{
  "name": "contributte/nextras-orm-events",
  "description": "Doctrine-like events for Nextras ORM entity lifecycle",
  "keywords": [
    "nextras",
    "orm",
    "events",
    "lifecycle",
    "doctrine"
  ],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/contributte/nextras-orm-events",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": ">=7.2",
    "contributte/utils": "^0.5.2",
    "nette/di": "^3.0.0",
    "nextras/orm": "^4.0"
  },
  "require-dev": {
    "mockery/mockery": "^1.3.3",
    "nette/caching": "^3.1",
    "ninjify/nunjuck": "^0.4",
    "ninjify/qa": "^0.12",
    "phpstan/phpstan": "^1.0",
    "phpstan/phpstan-deprecation-rules": "^1.0",
    "phpstan/phpstan-nette": "^1.0",
    "phpstan/phpstan-strict-rules": "^1.0"
  },
  "conflict": {
    "nette/di": "<=3.0.0-RC"
  },
  "autoload": {
    "psr-4": {
      "Contributte\\Nextras\\Orm\\Events\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Cases\\": "tests/cases",
      "Tests\\Fixtures\\": "tests/fixtures"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.9.x-dev"
    }
  }
}
