{
  "name": "contributte/nextras-orm-events",
  "description": "Doctrine-like events for Nextras ORM entity lifecycle",
  "keywords": [
    "nextras",
    "orm",
    "events",
    "lifecycle",
    "doctrine"
  ],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/contributte/nextras-orm-events",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": "^7.2",
    "nette/di": "~3.0.0",
    "nette/reflection": "~2.4.2",
    "nextras/orm": "^3.1.0"
  },
  "require-dev": {
    "mockery/mockery": "^0.9.9",
    "nette/caching": "~3.0.0",
    "ninjify/qa": "^0.8.0",
    "ninjify/nunjuck": "^0.2.0",
    "phpstan/extension-installer": "^1.0",
    "phpstan/phpstan-deprecation-rules": "^0.11",
    "phpstan/phpstan-nette": "^0.11",
    "phpstan/phpstan-shim": "^0.11",
    "phpstan/phpstan-strict-rules": "^0.11"
  },
  "conflict": {
    "nette/di": "<=3.0.0-RC"
  },
  "autoload": {
    "psr-4": {
      "Contributte\\Nextras\\Orm\\Events\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\Fixtures\\": "tests/fixtures"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true
  },
  "extra": {
    "branch-alias": {
      "dev-develop": "0.6.x-dev"
    }
  }
}
