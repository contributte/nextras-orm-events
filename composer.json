{
  "name": "contributte/nextras-orm-events",
  "description": "Doctrine-like events for Nextras ORM entity lifecycle",
  "keywords": [
    "nextras",
    "orm",
    "events",
    "lifecycle",
    "doctrine"
  ],
  "type": "library",
  "license": "MIT",
  "homepage": "https://github.com/contributte/nextras-orm-events",
  "authors": [
    {
      "name": "Milan Felix Å ulc",
      "homepage": "https://f3l1x.io"
    }
  ],
  "require": {
    "php": ">=8.0",
    "contributte/utils": "^0.5.2",
    "nette/di": "^3.0.0",
    "nextras/orm": "^4.0"
  },
  "require-dev": {
    "mockery/mockery": "^1.6.6",
    "nette/caching": "^3.2.3",
    "contributte/qa": "^0.4",
    "contributte/tester": "^0.3",
    "contributte/phpstan": "^0.1"
  },
  "autoload": {
    "psr-4": {
      "Contributte\\Nextras\\Orm\\Events\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests"
    }
  },
  "minimum-stability": "dev",
  "prefer-stable": true,
  "config": {
    "sort-packages": true,
    "allow-plugins": {
      "dealerdirect/phpcodesniffer-composer-installer": true
    }
  },
  "extra": {
    "branch-alias": {
      "dev-master": "0.9.x-dev"
    }
  }
}
